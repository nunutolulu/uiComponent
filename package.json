{
  "name": "my-kit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:lib": "vite build --config ./build/lib.config.ts",
    "build:docs": "vite build --config ./build/doc.config.ts && node script/copyDir.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "preview": "vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0 {src,mock} /**/*/{vue,ts,tsx} --fix",
    "lint:prettier": "prettier --writer **/*.{js,json,tsx,css,less,scss,vue,html,md}",
    "prepare": "husky install",
    "lint:lint-staged": "lint-staged",
    "docs:preview": "vitepress preview docs",
    "gen": "node ./script/genNewComp/index.js"
  },
  "dependencies": {
    "inquirer": "^9.2.12",
    "less-loader": "^11.1.3",
    "node-fetch": "^2.6.0",
    "vite-plugin-dts": "^3.6.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@eslint/create-config": "^0.4.6",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.1.1",
    "prismjs": "^1.29.0",
    "stylelint": "^16.0.2",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.10",
    "vite-plugin-md": "^0.22.5",
    "vitepress": "1.0.0-rc.32",
    "vue": "^3.3.12",
    "vue-tsc": "^1.8.25"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
